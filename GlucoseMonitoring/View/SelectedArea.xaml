<UserControl x:Class="GlucoseMonitoring.View.SelectedArea"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GlucoseMonitoring.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">


    <Canvas Name="SACanvas" Background="Transparent" ClipToBounds="True">
        <Border Name="SAborder" BorderBrush="{DynamicResource HighlightBrush}" BorderThickness="3" CornerRadius="5" Canvas.Left="110" Canvas.Top="110" MinWidth="50" MinHeight="50" Margin="0" Tag="Select"/>
        <Rectangle x:Name="SARec" Stroke="{DynamicResource AccentColorBrush}" RadiusX="5" RadiusY="5" StrokeThickness="3" Margin="0" Width="100" Height="100" />



    </Canvas>
</UserControl>
